    9  cal 3 $(date +%Y)
   10  sudo apt install ncal
   11  echo "Today is $(date +%A), $(date +%B), $(date +%Y)"
   12  echo "The time now is $(date +%H:%M:%S)"
   13  cal 3 $(date +%Y)
   14  cal 8 $(date +%Y)
   15  cal 8 2025
   16  history
