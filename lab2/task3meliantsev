[?2004l[?1049h[22;0;0t[?1h=[4mprintf[24m(3)                         Library Functions Manual                        [4mprintf[24m(3)[m
[m
[1mNAME[0m[m
       printf,  fprintf, dprintf, sprintf, snprintf, vprintf, vfprintf, vdprintf, vsprintf,[m
       vsnprintf - formatted output conversion[m
[m
[1mLIBRARY[0m[m
       Standard C library ([4mlibc[24m, [4m-lc[24m)[m
[m
[1mSYNOPSIS[0m[m
       [1m#include <stdio.h>[0m[m
[m
       [1mint printf(const char *restrict [4m[22mformat[24m[1m, ...);[0m[m
       [1mint fprintf(FILE *restrict [4m[22mstream[24m[1m,[0m[m
                   [1mconst char *restrict [4m[22mformat[24m[1m, ...);[0m[m
       [1mint dprintf(int [4m[22mfd[24m[1m,[0m[m
                   [1mconst char *restrict [4m[22mformat[24m[1m, ...);[0m[m
       [1mint sprintf(char *restrict [4m[22mstr[24m[1m,[0m[m
                   [1mconst char *restrict [4m[22mformat[24m[1m, ...);[0m[m
       [1mint snprintf(char [4m[22mstr[24m[1m[restrict .[4m[22msize[24m[1m], size_t [4m[22msize[24m[1m,[0m[m
                   [1mconst char *restrict [4m[22mformat[24m[1m, ...);[0m[m
[m
       [1mint vprintf(const char *restrict [4m[22mformat[24m[1m, va_list [4m[22map[24m[1m);[0m[m
       [1mint vfprintf(FILE *restrict [4m[22mstream[24m[1m,[0m[m
                   [1mconst char *restrict [4m[22mformat[24m[1m, va_list [4m[22map[24m[1m);[0m[m
       [1mint vdprintf(int [4m[22mfd[24m[1m,[0m[m
                   [1mconst char *restrict [4m[22mformat[24m[1m, va_list [4m[22map[24m[1m);[0m[m
       [1mint vsprintf(char *restrict [4m[22mstr[24m[1m,[0m[m
                   [1mconst char *restrict [4m[22mformat[24m[1m, va_list [4m[22map[24m[1m);[0m[m
       [1mint vsnprintf(char [4m[22mstr[24m[1m[restrict .[4m[22msize[24m[1m], size_t [4m[22msize[24m[1m,[0m[m
                   [1mconst char *restrict [4m[22mformat[24m[1m, va_list [4m[22map[24m[1m);[0m[m
[m
   Feature Test Macro Requirements for glibc (see [1mfeature_test_macros[22m(7)):[m
[m
       [1msnprintf[22m(), [1mvsnprintf[22m():[m
           _XOPEN_SOURCE >= 500 || _ISOC99_SOURCE[m
